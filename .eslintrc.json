{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
          ],
        "import/extensions": [
            "error",
            "ignorePackages"
            ],
        "class-methods-use-this": 0,
        "no-underscore-dangle": 0,
        "eqeqeq": 0,
        "no-plusplus": 0
    }
}